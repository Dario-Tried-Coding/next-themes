{
  "name": "react",
  "version": "0.0.0",
  "description": "A typesafe abstraction for advanced theming in your React apps.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc"
  },
  "keywords": [
    "theming",
    "typescript",
    "react",
    "next.js"
  ],
  "author": "dariotriedcoding",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dario-Tried-Coding/next-themes"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    }
  }
}